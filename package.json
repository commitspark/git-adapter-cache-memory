{
  "name": "@commitspark/git-adapter-cache-memory",
  "version": "0.5.1",
  "description": "In-memory caching proxy adapter for use in front of other Commitspark Git adapters",
  "license": "ISC",
  "private": false,
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist/**",
    "src/**",
    "README.md",
    "LICENSE",
    "package.json",
    "CHANGELOG.md"
