{
  "name": "@commitspark/git-adapter-cache-memory",
  "version": "0.6.0",
  "description": "In-memory caching proxy adapter for use in front of other Commitspark Git adapters",
  "license": "ISC",
  "private": false,
  "type": "module",
  "files": [
    "dist/**",
    "src/**",
    "README.md",
    "LICENSE",
    "package.json",
    "CHANGELOG.md"
  ],
  "exports": {
    ".": {
      "types": "./dist/types/index.d.ts",
      "require": "./dist/cjs/index.js",
      "import": "./dist/esm/index.js",
      "default": "./dist/esm/index.js"
    }
  },
  "scripts": {
    "build": "npm run build:cjs && npm run build:esm && npm run build:types",
    "build:cjs": "tsc --project tsconfig.build.cjs.json && echo '{\"type\": \"commonjs\"}' > dist/cjs/package.json",
    "build:esm": "tsc --project tsconfig.build.esm.json",
    "build:types": "tsc --project tsconfig.build.types.json",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "lint": "eslint \"{src,tests}/**/*.ts\"",
    "test": "jest"
  },
  "dependencies": {
    "@commitspark/git-adapter": "^0.13.0"
  },
  "devDependencies": {
    "@types/jest": "^29.5.10",
    "@types/node": "^22.14.0",
    "eslint": "^9.24.0",
    "jest": "^29.7.0",
    "jest-mock-extended": "^3.0.5",
    "prettier": "^3.1.0",
    "ts-jest": "^29.1.0",
    "ts-loader": "^9.3.1",
    "ts-node": "^10.9.2",
    "typescript": "^5.0.0",
    "typescript-eslint": "^8.29.0"
  }
}
